{
  "manifest_version": 3,
  "name": "WhatsApp Categories",
  "version": "0.0.1",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "use_dynamic_url": true,
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "**/*",
        "*"
      ]
    }
  ]
}
